<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="AE05">
	<class name="Libro" table="libros">
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<property name="titulo" type="string" column="titulo"/>
		<property name="autor" type="string" column="autor"/>
		<property name="anyoNacimiento" type="string" column="anyoNacimiento"/>
		<property name="anyoPublicacion" type="string" column="anyoPublicacion"/>
		<property name="editorial" type="string" column="editorial"/>
		<property name="numPaginas" type="string" column="numPaginas"/>
	</class>
</hibernate-mapping>
